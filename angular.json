{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-http": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/angular-http",
      "sourceRoot": "projects/angular-http/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-http",
            "index": "projects/angular-http/src/index.html",
            "main": "projects/angular-http/src/main.ts",
            "polyfills": "projects/angular-http/src/polyfills.ts",
            "tsConfig": "projects/angular-http/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/angular-http/src/favicon.ico",
              "projects/angular-http/src/assets"
            ],
            "styles": [
              "projects/angular-http/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/angular-http/src/environments/environment.ts",
                  "with": "projects/angular-http/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-http:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-http:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-http:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular-http/src/test.ts",
            "polyfills": "projects/angular-http/src/polyfills.ts",
            "tsConfig": "projects/angular-http/tsconfig.spec.json",
            "karmaConfig": "projects/angular-http/karma.conf.js",
            "assets": [
              "projects/angular-http/src/favicon.ico",
              "projects/angular-http/src/assets"
            ],
            "styles": [
              "projects/angular-http/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/angular-http/tsconfig.app.json",
              "projects/angular-http/tsconfig.spec.json",
              "projects/angular-http/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/angular-http/e2e/protractor.conf.js",
            "devServerTarget": "angular-http:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-http:serve:production"
            }
          }
        }
      }
    },
